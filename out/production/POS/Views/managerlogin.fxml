<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="708" prefWidth="1366" style="-fx-background-image: url('file:src/pictures/pic1.jpg'); -fx-background-size: cover; -fx-background-size: 100% 100%;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ManagerController">
   <children>
      <Button fx:id="back" layoutX="85.0" layoutY="621.0" mnemonicParsing="false" onAction="#back" prefHeight="37.0" prefWidth="83.0" text="Back">
         <font>
            <Font name="Andalus" size="18.0" />
         </font>
         <effect>
            <Lighting>
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant />
               </light>
            </Lighting>
         </effect>
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor>
      </Button>
      <PasswordField fx:id="passmanager" layoutX="544.0" layoutY="375.0" prefHeight="50.0" prefWidth="273.0" promptText=" Password ">
         <font>
            <Font name="Andalus" size="18.0" />
         </font>
         <effect>
            <InnerShadow />
         </effect></PasswordField>
      <TextField fx:id="usermanager" layoutX="544.0" layoutY="227.0" prefHeight="50.0" prefWidth="273.0" promptText=" User Name ">
         <font>
            <Font name="Andalus" size="18.0" />
         </font>
         <effect>
            <InnerShadow />
         </effect></TextField>
      <Button fx:id="addmanager" layoutX="1129.0" layoutY="54.0" mnemonicParsing="false" onAction="#gotoeditmanager" prefHeight="48.0" prefWidth="174.0" text="EDIT MANAGER ">
         <font>
            <Font name="Andalus" size="18.0" />
         </font>
         <effect>
            <Lighting>
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant />
               </light>
            </Lighting>
         </effect>
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor>
      </Button>
      <Button fx:id="login" layoutX="639.0" layoutY="517.0" mnemonicParsing="false" onAction="#login" prefHeight="48.0" prefWidth="83.0" text="Login">
         <font>
            <Font name="Andalus" size="18.0" />
         </font>
         <effect>
            <Lighting>
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant />
               </light>
            </Lighting>
         </effect>
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
