<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="708" prefWidth="1366" style="-fx-background-image: url('file:src/zpictures/pic6.png'); -fx-background-size: cover; -fx-background-size: 100% 100%;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ManagerController">
   <children>
      <TextField fx:id="muserfield" layoutX="567.0" layoutY="103.0" prefHeight="45.0" prefWidth="230.0" promptText="UserName">
         <font>
            <Font name="Andalus" size="18.0" />
         </font>
         <effect>
            <InnerShadow />
         </effect></TextField>
      <PasswordField fx:id="password" layoutX="568.0" layoutY="297.0" prefHeight="45.0" prefWidth="230.0" promptText="Password">
         <font>
            <Font name="Andalus" size="18.0" />
         </font>
         <effect>
            <InnerShadow />
         </effect></PasswordField>
      <PasswordField fx:id="confirmpassword" layoutX="568.0" layoutY="396.0" prefHeight="45.0" prefWidth="230.0" promptText="Confirm Password">
         <font>
            <Font name="Andalus" size="18.0" />
         </font>
         <effect>
            <InnerShadow />
         </effect></PasswordField>
      <Button layoutX="85.0" layoutY="621.0" mnemonicParsing="false" onAction="#back1" prefHeight="37.0" prefWidth="83.0" text="Back">
         <font>
            <Font name="Andalus" size="18.0" />
         </font>
         <effect>
            <Lighting>
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant />
               </light>
            </Lighting>
         </effect>
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor></Button>
      <Button layoutX="641.0" layoutY="496.0" mnemonicParsing="false" onAction="#editmanager" prefHeight="42.0" prefWidth="83.0" text="Edit">
         <font>
            <Font name="Andalus" size="18.0" />
         </font>
         <effect>
            <Lighting>
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant />
               </light>
            </Lighting>
         </effect>
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor>
      </Button>
      <PasswordField fx:id="oldpassword" layoutX="567.0" layoutY="200.0" prefHeight="45.0" prefWidth="230.0" promptText=" Old Password">
         <font>
            <Font name="Andalus" size="18.0" />
         </font>
         <effect>
            <InnerShadow />
         </effect>
      </PasswordField>
       
   </children>
</AnchorPane>
